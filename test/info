function create() {
  for (let i = 0; i<225; i++) {
  const div = document.createElement('div')
    div.classList.add('cell')
    board.appendChild(div)
  }
  function test() {
    const cell = document.querySelectorAll('.cell')

    for (let i = 0; i<225; i++) {
